{
  "name": "webdriverjs-framework",
  "version": "1.0.0",
  "description": "Basic Test Automation Framework based on selenium-webdriver, ESM for ES6 sintax and the Allure Reporter",
  "main": "index.js",
  "scripts": {
    "test": "mocha --no-timeouts -r esm",
    "report": "allure generate --clean && allure open"
